#include <stdio.h>

int main (int argc, char* argv[]) 
{

	char char_A = 'A';
	char char_B = 'B';
 
	char * const myPtr = &char_A;
	//myPtr = &char_B;    // error - can't change address of myPtr

	int i = 2;

	switch (i) 
	{
		case 0: printf("0\n");
		case 1: printf("1\n");
		case 2: printf("2\n");
		default: printf("x\n");
	}

	return 0;
}
